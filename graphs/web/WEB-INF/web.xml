<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>HelloServlet</servlet-name>
        <servlet-class>HelloServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HelloServlet</servlet-name>
        <url-pattern>/hello</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>TestListServlet</servlet-name>
        <servlet-class>TestListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TestListServlet</servlet-name>
        <url-pattern>/test1</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>GetDataTotalClTr</servlet-name>
        <servlet-class>GetDataTotalClTr</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetDataTotalClTr</servlet-name>
        <url-pattern>/totalClosedTransactions</url-pattern>
    </servlet-mapping>

</web-app>

